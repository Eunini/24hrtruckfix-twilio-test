# Environment Variables
# Copy this file to .env and update with your actual values

# Twilio Configuration
TWILIO_ACCOUNT_SID=your_account_sid_here
# Support for multiple auth tokens (comma-separated) for key rotation
TWILIO_AUTH_TOKEN=your_primary_auth_token_here,your_backup_auth_token_here

# Server Configuration
PORT=3000

# Example with actual format (replace with your real values):
# TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# TWILIO_AUTH_TOKEN=your_primary_token_32_chars,your_backup_token_32_chars